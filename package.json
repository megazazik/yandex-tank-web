{
  "name": "yandex-tank-service",
  "version": "0.1.0",
  "description": "Web interface for yandex tank",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "start-watch": "concurrently --kill-others \"nodemon --watch dist/ node dist/server/index.js\" \"npm run build-server -- --watch\"",
    "build": "npm run build-client && npm run build-server",
    "build-client": "megazazik-build -c ./config/webpack.config.js",
    "build-server": "tsc -P tsconfig.server.json",
    "build-docker": "docker build --rm -t yandex-tank-web --build-arg IMAGE=my-tank .",
    "docker": "docker run -v $(pwd)/tank:/var/loadtest -p 4000:4000 -it yandex-tank-web",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "cross-env NODE_ENV=production npm run"
  },
  "keywords": [
    "yandex",
    "tank",
    "performance",
    "stress",
    "test"
  ],
  "author": "Mikhail Lebedev <megazazik@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@hot-loader/react-dom": "^16.9.0",
    "@megazazik/build": "^0.2.0",
    "@storybook/react": "^5.1.11",
    "@types/express": "^4.17.1",
    "@types/minimist": "^1.2.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "typescript": "^3.6.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "cross-env": "^5.2.1",
    "encaps": "^0.8.0",
    "express": "^4.17.1",
    "minimist": "^1.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
