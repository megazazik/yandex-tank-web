{
  "name": "yandex-tank-web",
  "version": "0.1.1",
  "description": "Web interface for yandex tank",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "start-watch": "concurrently --kill-others \"nodemon --watch dist/ node dist/server/index.js\" \"npm run build-server -- --watch\"",
    "watch": "concurrently --kill-others \"nodemon --watch dist/ node dist/server/index.js\" \"npm run build-server -- --watch\" \"npm run build-client -- --watch\"",
    "build": "npm run build-client && npm run build-server",
    "build-client": "megazazik-build -c ./config/webpack.config.js",
    "build-server": "tsc -P tsconfig.server.json",
    "build-image": "docker build --rm -t yandex-tank-web --build-arg IMAGE=my-tank .",
    "docker": "docker run -v $(pwd)/.storage:/app/.storage -v $(pwd)/tank:/var/loadtest -p 5000:4000 -it yandex-tank-web",
    "test": "sh default -d src/**/*.spec.* -c tape",
    "tape": "ts-node -P tsconfig.tests.json node_modules/tape/bin/tape",
    "test-w": "nodemon -e js,jsx,ts,tsx,json --exec npm run test",
    "prod": "cross-env NODE_ENV=production npm run"
  },
  "keywords": [
    "yandex",
    "tank",
    "performance",
    "stress",
    "test"
  ],
  "author": "Mikhail Lebedev <megazazik@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@hot-loader/react-dom": "^16.10.2",
    "@megazazik/build": "^0.2.0",
    "@storybook/react": "^5.2.5",
    "@types/classnames": "^2.2.9",
    "@types/express": "^4.17.2",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/morgan": "^1.7.37",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-redux": "^7.1.5",
    "@types/sinon": "^7.5.0",
    "@types/tape": "^4.2.33",
    "@types/uuid": "^3.4.6",
    "@types/yaml": "^1.2.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "sinon": "^7.5.0",
    "tape": "^4.11.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@megazazik/validate": "^0.2.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.1",
    "deepmerge": "^4.2.2",
    "encaps": "^0.8.1",
    "express": "^4.17.1",
    "minimist": "^1.2.3",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "react": "^16.11.0",
    "react-cached-callback": "^2.0.1",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "reducer-assistant": "^0.3.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "uuid": "^3.3.3",
    "yaml": "^1.7.2"
  }
}